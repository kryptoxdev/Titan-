{
	"manifest_version": 3,
	"name": "Titan+",
	"description": "Fixing Titans problems because they won't!",
	"version": "0.6.9",
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["http://localhost/*"],
			"js": ["scripts/settings.js"]
		},
		{
			"matches": ["http://localhost/phonecheck"],
			"js": ["scripts/redColourFix.js", "scripts/switchBezelRear.js", "scripts/gradeNameChange.js", "scripts/processCounter.js"]
		}
	],
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"action": {
		"default_popup": "TitanPlus.html"
	}
}